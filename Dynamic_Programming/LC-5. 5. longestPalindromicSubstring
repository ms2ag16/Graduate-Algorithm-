def longestPalindrome(s):
    n=len(s)
    dp=[[0 for j in range(n)] for i in range(n)]
    #print dp
    tmp=0
    for i in range(n):
        dp[i][i]=1
    for i in range(n-1):
        if (s[i]==s[i+1]):
            dp[i][i + 1] =1+dp[i][i]
        else:
            dp[i][i + 1] =1


    return dp


if __name__=='__main__':
    s='dabad'
    print longestPalindrome(s)
